# Python Virtualenvs
alias mkvenv3.8='virtualenv -p python3.8 venv && source ./venv/bin/activate && pip install pylint'
alias mkvenv3.9='virtualenv -p python3.9 venv && source ./venv/bin/activate && pip install pylint'
alias mkvenv='virtualenv venv && source ./venv/bin/activate && pip install pylint'
